#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent {
        label 'master'
    }
    tools { 
        maven 'maven 3'
        jdk 'jdk9'
    }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("build") {
            steps {
                dir("testjen/maven/"){
                    sh 'mvn clean verify'
                }
            }
        }
    }
}
